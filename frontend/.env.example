# Frontend Environment Variables for TaskNest

# API Endpoints
REACT_APP_API_URL=http://localhost:5500/api/v1

# AWS S3 Configuration for Frontend Direct Uploads (if needed)
REACT_APP_AWS_BUCKET_NAME=tasknest-files
REACT_APP_AWS_REGION=us-east-1

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=false
REACT_APP_ENABLE_REALTIME=false

# Analytics (if needed for future)
REACT_APP_ANALYTICS_ID=

# Miscellaneous
REACT_APP_MAX_FILE_SIZE=5242880
